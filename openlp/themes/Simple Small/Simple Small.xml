<?xml version="1.0" encoding="utf-8"?>
<theme version="2.0">
    <name>Simple Small</name>
    <background type="solid">
        <color>#000000</color>
    </background>
    <font type="main">
        <name>Arial</name>
        <color>#FFFFFF</color>
        <size>27</size>
        <bold>True</bold>
        <italics>False</italics>
        <line_adjustment>1</line_adjustment>
        <location height="700" override="True" width="950" x="50" y="90"/>
        <shadow shadowColor="#000000" shadowSize="5">True</shadow>
        <outline outlineColor="#000000" outlineSize="2">False</outline>
    </font>
    <font type="footer">
        <name>Arial Black</name>
        <color>#FFFFFF</color>
        <size>33</size>
        <bold>False</bold>
        <italics>False</italics>
        <line_adjustment>0</line_adjustment>
        <location height="76" override="True" width="1004" x="50" y="5"/>
        <shadow shadowColor="#000000" shadowSize="5">True</shadow>
        <outline outlineColor="#000000" outlineSize="2">False</outline>
    </font>
    <display>
        <horizontalAlign>0</horizontalAlign>
        <verticalAlign>0</verticalAlign>
        <slideTransition>False</slideTransition>
    </display>
</theme>
